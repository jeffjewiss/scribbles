{
  "name": "scribbles",
  "description": "A minimal theme for ghost. Text focused.",
  "version": "0.0.0",
  "engines": {
    "ghost-api": "v2"
  },
  "scripts": {
    "start": "gulp",
    "scan": "gscan .",
    "zip": "gulp zip"
  },
  "license": "MIT",
  "author": {
    "name": "Jeff Jewiss",
    "email": "jeff@jeffjewiss.com",
    "url": "jeffjewiss.com"
  },
